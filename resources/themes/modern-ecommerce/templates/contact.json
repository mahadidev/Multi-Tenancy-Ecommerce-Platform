{
  "page": {
    "title": "Contact Us",
    "slug": "contact",
    "type": "page",
    "seo": {
      "meta_title": "Contact Us - Modern Store",
      "meta_description": "Get in touch with our team. We're here to help with any questions about our products or services.",
      "meta_keywords": "contact us, customer support, help, questions"
    }
  },
  "sections": [
    {
      "id": "page-header",
      "name": "Page Header",
      "type": "header",
      "settings": {
        "background_color": "gray-50",
        "padding_top": "40px",
        "padding_bottom": "40px"
      },
      "components": [
        {
          "type": "breadcrumb",
          "content": {
            "items": [
              { "label": "Home", "url": "/" },
              { "label": "Contact", "url": "/contact", "active": true }
            ]
          }
        },
        {
          "type": "heading",
          "content": {
            "text": "Contact Us",
            "tag": "h1",
            "size": "3xl",
            "color": "gray-900",
            "weight": "bold",
            "margin_top": "4"
          }
        },
        {
          "type": "text",
          "content": {
            "text": "We'd love to hear from you. Send us a message and we'll respond as soon as possible.",
            "size": "lg",
            "color": "gray-600",
            "margin_top": "2"
          }
        }
      ]
    },
    {
      "id": "contact-info",
      "name": "Contact Information",
      "type": "content",
      "settings": {
        "background_color": "white",
        "padding_top": "60px",
        "padding_bottom": "60px"
      },
      "components": [
        {
          "type": "contact-info-cards",
          "content": {
            "cards": [
              {
                "icon": "phone",
                "title": "Phone",
                "content": "+1 (555) 123-4567",
                "link": "tel:+15551234567"
              },
              {
                "icon": "email",
                "title": "Email",
                "content": "support@modernstore.com",
                "link": "mailto:support@modernstore.com"
              },
              {
                "icon": "location",
                "title": "Address",
                "content": "123 Business St, Suite 100\nNew York, NY 10001",
                "link": "https://maps.google.com"
              },
              {
                "icon": "clock",
                "title": "Business Hours",
                "content": "Monday - Friday: 9:00 AM - 6:00 PM\nSaturday: 10:00 AM - 4:00 PM\nSunday: Closed"
              }
            ],
            "columns": 4
          }
        }
      ]
    },
    {
      "id": "contact-form-section",
      "name": "Contact Form",
      "type": "content",
      "settings": {
        "background_color": "gray-50",
        "padding_top": "80px",
        "padding_bottom": "80px"
      },
      "components": [
        {
          "type": "two-column-content",
          "content": {
            "left_column": {
              "content": [
                {
                  "type": "heading",
                  "content": {
                    "text": "Send us a Message",
                    "tag": "h2",
                    "size": "2xl",
                    "color": "gray-900",
                    "weight": "bold"
                  }
                },
                {
                  "type": "contact-form",
                  "content": {
                    "fields": [
                      {
                        "type": "text",
                        "name": "name",
                        "label": "Full Name",
                        "placeholder": "Your full name",
                        "required": true,
                        "width": "full"
                      },
                      {
                        "type": "email",
                        "name": "email",
                        "label": "Email Address",
                        "placeholder": "your.email@example.com",
                        "required": true,
                        "width": "half"
                      },
                      {
                        "type": "tel",
                        "name": "phone",
                        "label": "Phone Number",
                        "placeholder": "+1 (555) 123-4567",
                        "required": false,
                        "width": "half"
                      },
                      {
                        "type": "select",
                        "name": "subject",
                        "label": "Subject",
                        "required": true,
                        "width": "full",
                        "options": [
                          { "value": "general", "label": "General Inquiry" },
                          { "value": "support", "label": "Customer Support" },
                          { "value": "order", "label": "Order Question" },
                          { "value": "returns", "label": "Returns & Refunds" },
                          { "value": "partnership", "label": "Partnership" },
                          { "value": "other", "label": "Other" }
                        ]
                      },
                      {
                        "type": "textarea",
                        "name": "message",
                        "label": "Message",
                        "placeholder": "Please describe how we can help you...",
                        "required": true,
                        "width": "full",
                        "rows": 6
                      }
                    ],
                    "submit_button": {
                      "text": "Send Message",
                      "style": "primary"
                    },
                    "success_message": "Thank you for your message! We'll get back to you within 24 hours.",
                    "error_message": "Sorry, there was an error sending your message. Please try again.",
                    "margin_top": "6"
                  }
                }
              ]
            },
            "right_column": {
              "content": [
                {
                  "type": "heading",
                  "content": {
                    "text": "Visit Our Store",
                    "tag": "h3",
                    "size": "xl",
                    "color": "gray-900",
                    "weight": "semibold"
                  }
                },
                {
                  "type": "map",
                  "content": {
                    "address": "123 Business St, Suite 100, New York, NY 10001",
                    "zoom": 15,
                    "height": "300px",
                    "marker": true,
                    "margin_top": "4"
                  }
                },
                {
                  "type": "heading",
                  "content": {
                    "text": "FAQ",
                    "tag": "h3",
                    "size": "xl",
                    "color": "gray-900",
                    "weight": "semibold",
                    "margin_top": "8"
                  }
                },
                {
                  "type": "faq-list",
                  "content": {
                    "questions": [
                      {
                        "question": "What are your shipping options?",
                        "answer": "We offer standard (3-5 days), expedited (2-3 days), and overnight shipping."
                      },
                      {
                        "question": "What is your return policy?",
                        "answer": "We accept returns within 30 days of purchase for a full refund or exchange."
                      },
                      {
                        "question": "Do you ship internationally?",
                        "answer": "Yes, we ship to over 50 countries worldwide. Shipping costs vary by location."
                      }
                    ],
                    "margin_top": "4"
                  }
                }
              ]
            }
          }
        }
      ]
    }
  ]
}