{
    "compilerOptions": {
        "target": "es2016",
        "jsx": "react",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "paths": {
            "@/*": ["./resources/js/*"]
        },
        "resolveJsonModule": true
    },
    "include": ["./resources/js"]
}
