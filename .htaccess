 <IfModule mod_rewrite.c>
    RewriteEngine On

    # Block any access to /public/ URLs
    RewriteCond %{REQUEST_URI} ^/public/
    RewriteRule ^.*$ - [F,L]

    # Default Laravel rewrite rule to handle routing
    RewriteRule ^(.*)$ public/$1 [L]
</IfModule>
